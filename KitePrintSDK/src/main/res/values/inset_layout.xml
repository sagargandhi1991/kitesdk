<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="single_column_inset_layout">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="double_column_inset_layout.container" parent="">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">0.5</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="double_column_inset_layout.container.left"/>

    <style name="double_column_inset_layout.container.right"/>


    <style name="double_column_inset_layout.content" parent="">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="double_column_inset_layout.content.left">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="double_column_inset_layout.content.right">
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>

</resources>
