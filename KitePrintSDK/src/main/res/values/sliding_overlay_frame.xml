<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlidingOverlayFrame">
        <attr name="expandDirection">
            <enum name="up"    value="0"/>
            <enum name="down"  value="1"/>
            <enum name="left"  value="2"/>
            <enum name="right" value="3"/>
        </attr>
        <attr name="alwaysVisibleView" format="reference"/>
        <attr name="revealedView" format="reference"/>
        <attr name="slideAnimationDurationMillis" format="integer"/>
    </declare-styleable>
</resources>